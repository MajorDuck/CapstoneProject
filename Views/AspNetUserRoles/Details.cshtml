@using Microsoft.AspNetCore.Identity
@model IdentityUserRole<string>

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>User Role</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            User Id:
        </dt>
        <dd class="col-sm-10">
            @Model.UserId
        </dd>
        <dt class="col-sm-2">
            Role Id:
        </dt>
        <dd class="col-sm-10">
            @Model.RoleId
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.UserId" asp-route-roleId="@Model.RoleId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
