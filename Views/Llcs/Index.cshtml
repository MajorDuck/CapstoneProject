@model IEnumerable<Capstone.Models.Llc>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.LlcName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LlcID)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.LlcName)
            </td>
            
            <td>
                @Html.DisplayFor(modelItem => item.LlcID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.LlcID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.LlcID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.LlcID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
